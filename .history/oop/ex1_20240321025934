class Employee():
    def __init__(self, emp_id, emp_name, emp_salary, emp_department):
        self.emp_id=emp_id
        self.emp_name=emp_name
        self.emp_salary=emp_salary
        self.emp_department=emp_department
        self.hours_worked

    def calculate_emp_salary(self, salary, hours_worked):
        self.overtime=self.hours_worked - 50
        self.overtime_sal= (self.overtime * (self.emp_salary/50))
        self.emp_salary+=self.overtime_sal

    def assign_department(self, department):
        self.emp_department=department
        print('Department changed successfully.')

    def print_emp_details(self):
        print('Employee Details:')
        print('ID: ',self.emp_id)
        print('Name: ',self.emp_name)
        print('Salary: ',self.emp_salary)
        print('Department: ',self.emp_department)


e1=Employee('E7876', 'Adams', 50000, 'Accounting')
        